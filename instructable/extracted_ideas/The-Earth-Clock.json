        "materials_required": [
            "Warm white SMD LED (3200K)",
            "Clear plastic ball (\u2300100mm)",
            "5mm plywood",
            "Arduino Uno",
            "5V regulator",
            "PWM signal module",
            "Stepper motors (28BYJ-48)",
            "ULN2003 driver",
            "Clock module",
            "Protoboard",
            "10k potentiometer",
            "1418 diode",
            "100nF ceramic capacitors",
            "555 timer chip",
            "2 screw-connectors",
            "3 pins dupont connector",
            "Dark oak stain",
            "Varnish",
            "Earth origami (printed)",
            "Acrylic paint",
            "LEGO pieces",
            "Brass tube"
